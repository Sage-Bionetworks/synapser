\name{synGetEvaluation}
\alias{synGetEvaluation}
\docType{methods}
\title{
synGetEvaluation
}
\description{
Gets an Evaluation object from Synapse.
}
\usage{
synGetEvaluation(id)
}
\arguments{
\item{id}{  The ID of the Evaluation to return.}
}
\value{
 an Evaluation object
}
\examples{
\dontrun{
eval <- synGetEvaluation(345)
}
}


