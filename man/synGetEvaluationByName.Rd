\name{synGetEvaluationByName}
\alias{synGetEvaluationByName}
\docType{methods}
\title{
synGetEvaluationByName
}
\description{
Gets an Evaluation object from Synapse.
}
\usage{
synGetEvaluationByName(name)
}
\arguments{
\item{name}{  The name of the Evaluation to return.}
}
\value{
 an Evaluation object
}
\examples{
\dontrun{
synGetEvaluationByName("my_evaluation")
}
}


